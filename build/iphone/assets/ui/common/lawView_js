function lawView(e){function t(e){var t=require("ui/common/QuestionView");questions=new t(e),a.add(questions)}var i="/images/law/",a=Ti.UI.createView({backgroundImage:"/images/mainmenu/background.png"}),r=[];switch(e){case"law":r=["PERSONAL INJURY","BANKRUPTCY","DUI","TAX","LEGAL DOCUMENTS"];break;case"date":r=["ENTERTAINMENT","DINING","TRAVEL","FAMILY","HOME/AUTO"];break;case"health":r=["SUPPLEMENTAL","HEALTH","LIFE","DISABILITY","LONG TERM CARE"]}var o=(Ti.UI.createView({height:Ti.UI.FILL,width:Ti.UI.FILL,backgroundColor:"#333",opacity:.7}),{fontFamily:"HelveticaNeue-Thin",fontSize:"21dp",color:"#fff"}),n=Ti.UI.createImageView({image:i+"home.png",left:"12.5dp",center:{y:"50dp"}}),d=(Ti.UI.createImageView({image:i+"menu.png",right:"12.5dp",center:{y:"50dp"}}),Ti.UI.createImageView({image:i+"help.png",left:"12.5dp",bottom:"15dp"}),Ti.UI.createImageView({image:i+"profile.png",right:"12.5dp",bottom:"15dp"}),Ti.UI.createImageView({image:i+e+"R.png",left:"0dp",center:{y:"50%"}})),c=Ti.UI.createView({height:"416dp",layout:"vertical",center:{y:"50%"},right:"-320dp"}),p=Ti.UI.createView({backgroundImage:i+e+"1.png",right:"0dp",bottom:"36.5",width:"222dp",height:"54dp",text:r[0]}),l=Ti.UI.createView({backgroundImage:i+e+"2.png",right:"0dp",bottom:"36.5",width:"156dp",height:"54dp",text:r[1]}),s=Ti.UI.createView({backgroundImage:i+e+"3.png",right:"0dp",bottom:"36.5",width:"119.5",height:"54dp",text:r[2]}),u=Ti.UI.createView({backgroundImage:i+e+"4.png",right:"0dp",bottom:"36.5",width:"156dp",height:"54dp",text:r[3]}),g=Ti.UI.createView({backgroundImage:i+e+"5.png",right:"0dp",width:"222dp",height:"54dp",text:r[4]}),f=Ti.UI.createLabel({text:r[0],font:o,color:"white",center:{x:"116dp"}}),T=Ti.UI.createLabel({text:r[1],font:o,color:"white",center:{x:"83dp"}}),m=Ti.UI.createLabel({text:r[2],font:o,color:"white",center:{x:"68dp"}}),h=Ti.UI.createLabel({text:r[3],font:o,color:"white",center:{x:"83dp"}}),I=Ti.UI.createLabel({text:r[4],font:o,color:"white",center:{x:"116dp"}});p.ad