function SlideOutMenu(){function e(){var e=Ti.UI.createTableViewRow({height:"75dp",backgroundColor:"#23b823"}),t=Ti.UI.createLabel({text:"Main Menu",font:i,left:"30dp",color:"white"});e.add(t),e.add(d),tableData.push(e);for(var n=0;n<data.length;n++){rowData=data[n];var o=Ti.UI.createTableViewRow({height:"50dp",link:rowData.link,backgroundColor:"#efefef"}),c=Ti.UI.createImageView({image:r+rowData.icon,center:{x:"35dp"},link:rowData.link}),s=Ti.UI.createLabel({text:rowData.title,font:a,left:"70dp",link:rowData.link});o.add(c),o.add(s),tableData.push(o)}l.setData(tableData)}function t(){var e=Ti.UI.createView({height:Ti.UI.SIZE,width:"71%",right:"0dp",layout:"vertical"}),t=Ti.UI.createView({height:"10dp"}),i=n.createButton("Logout","#b40301"),a=n.createButton("Back To App","#23b823");o.add(p),e.add(i),e.add(a),e.add(t),o.add(e),a.addEventListener("click",function(){o.remove(e),o.remove(p)}),i.addEventListener("click",function(){Ti.App.Properties.setBool("loggedIn",!1),Ti.fireEvent("load"),o.remove(e),o.remove(p)})}var i={fontFamily:"HelveticaNeue-Thin",fontSize:"28dp",color:"#fff"},a={fontFamily:"HelveticaNeue-Thin",fontSize:"22dp",color:"#fff"};data=[{icon:"profile.png",title:"Profile",link:"openProfile"},{icon:"inbox.png",title:"Inbox",link:"openInbox"},{icon:"interest.png",title:"Interests",link:"openInterests"},{icon:"signOut.png",title:"Sign Out",link:"logOut"}];var r="/images/profile/",n=require("ui/common/buttonCreator"),o=Ti.UI.createView({backgroundColor:"#fff",height:Ti.Platform.displayCaps.platformHeight,width:.77*Ti.Platform.displayCaps.platformWidth,left:-(.77*Ti.Platform.displayCaps.platformWidth),opacity:.9});o.animate({left:"0dp",duration:750});var d=Ti.UI.createView({height:"30dp",width:"40dp",right:"0dp"});tableData=[];var c=Ti.UI.createView({height:"20dp",top:"0dp",backgroundColor:"#1c971c",right:"0.5dp"}),l=Ti.UI.createTableView({height:"275dp",scrollable:!1,top:"20dp"});o.add(c),e(),o.add(l);var s=Ti.UI.createImageView({image:r+"arrow.png"});d.add(s);var p=Ti.UI.createView({height:Ti.UI.FILL,width:Ti.UI.FILL,backgroundColor:"#333",opacity:.7});return l.addEventListener("click",function(e){"logOut"==e.source.link?t():"logOut"!=e.source.link&&(o.animate({left:-(.77*Ti.Platform.displayCaps.platformWidth),duration:750},function(){o=null,Ti.fireEvent(e.source.link)}),Ti.fireEvent("closeSlideOut"))}),d.addEventListener("click",function(){o.animate({left:-(.77*Ti.Platform.displayCaps.platformWidth),duration:750}),setTimeout(function(){o=null,Ti.fireEvent("closeSlideOut")},750)}),o}module.exports=SlideOutMenu;