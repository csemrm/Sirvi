function TutorialView(){var e=Ti.UI.createView({backgroundColor:"#00a2c5"}),t={fontFamily:"HelveticaNeue-Thin",fontSize:"28dp",color:"#fff"},i="/images/tutorial/",a=Ti.UI.createImageView({image:i+"arrow.png",left:"12.5dp",center:{y:"40dp"}}),r=Ti.UI.createImageView({image:i+"arrow.png",left:"12.5dp",center:{y:"40dp"}}),o=Ti.UI.createImageView({image:i+"tut1.png"}),n=Ti.UI.createImageView({image:i+"nextBlue.png",height:"47dp",width:"170.5dp"}),d=Ti.UI.createLabel({text:"More Please",font:t,color:"white"}),c=Ti.UI.createButton({height:"47dp",width:"170.5dp",bottom:"30dp"});c.add(n),c.add(d);var p=Ti.UI.createView({height:Ti.UI.FILL,width:"294dp",top:"55dp",opacity:0});p.add(o),p.add(c),e.add(p);var l=Ti.UI.createImageView({image:i+"5.png"}),s=Ti.UI.createImageView({image:i+"nextRed.png",height:"47dp",width:"170.5dp"}),u=Ti.UI.createLabel({text:"More Please",font:t,color:"white"}),f=Ti.UI.createButton({height:"47dp",width:"170.5dp",bottom:"30dp"});f.add(s),f.add(u);var g=Ti.UI.createView({height:Ti.UI.FILL,width:"294dp",top:"55dp",opacity:0});g.add(l),g.add(f);var T=Ti.UI.createImageView({image:i+"tut3t@2x.png",center:{y:"40%"}}),m=Ti.UI.createImageView({image:i+"nextYellow.png",height:"47dp",width:"170.5dp"}),h=Ti.UI.createLabel({text:"Let's Start",font:t,color:"white"}),I=Ti.UI.createButton({height:"47dp",width:"170.5dp",bottom:"30dp"});I.add(m),I.add(h);var w=Ti.UI.createView({height:Ti.UI.FILL,width:"294dp",top:"55dp",opacity:0});w.add(T),w.add(I);var v=Titanium.UI.createAnimation({opacity:0,duration:500}),U=Titanium.UI.createAnimation({opacity:1,duration:500});return p.animate(U),c.addEventListener("click",function(){e.add(g),p.animate(v,function(){e.remove(p),g.animate(U),e.add(a)}),e.animate({backgroundColor:"#ef3d3b",duration:500})}),f.addEventListener("click",function(){e.add(w),e.add(r),e.remove(a),g.animate(v,function(){e.remove(g),w.animate(U)}),e.animate({backgroundColor:"#f7b21d",duration:500})}),r.addEventListener("click",function(){e.add(g),e.add(a),e.r